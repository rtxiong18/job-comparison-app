<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="edu.gatech.seclass.jobcompare6300">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JobCompare6300"
        tools:targetApi="31">
        <activity
            android:name=".BaseActivity"
            android:exported="false" />
        <activity
            android:name=".AddJobOfferActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".OldCompareJobsActivity" />
        <activity android:name=".CurrentJobActivity" />
        <activity android:name=".JobOffersActivity" />
        <activity android:name=".JobSettingsActivity" />
        <activity android:name=".EditJobOfferActivity" />
        <activity android:name=".CompareJobsActivity" />
        <activity android:name=".CompareTwoJobsActivity" />
        <activity android:name=".DeveloperToolsActivity" />
    </application>

</manifest>